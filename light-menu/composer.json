{
    "name": "celepar/light-menu",
    "description": "Menu para integração com o Layout do Light-PHP para Laravel5.",
    "keywords": ["light-php", "menu", "laravel"],
    "author": "João Alfredo Knopik Junior",
    "require": {
        "php": ">=5.5.9",
        "tlr/menu": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Celepar\\Light\\Menu\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Celepar\\Light\\Menu\\Tests\\": "tests/"
        }
    }
}