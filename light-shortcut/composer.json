{
    "name": "celepar/light-shortcut",
    "description": "Shortcuts para integração com o Layout do Light-PHP para Laravel5.",
    "keywords": ["light-php", "shortcut", "laravel"],
    "author": "João Alfredo Knopik Junior",
    "require": {
      "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "Celepar\\Light\\Shortcut\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Celepar\\Light\\Shortcut\\Tests\\": "tests/"
        }
    }
}